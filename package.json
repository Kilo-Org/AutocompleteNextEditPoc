  "scripts": {
    "test": "npm run test:core && npm run test:harness",
    "test:core": "vitest run --config core/vitest.config.ts",
    "test:harness": "vitest run --config core/vscode-test-harness/vitest.config.ts",
    "test:watch": "vitest --config core/vitest.config.ts",
    "lint": "eslint core --ext ts",
    "lint:fix": "eslint core --ext ts --fix",
    "typecheck": "tsc --noEmit",
    "tsc:watch": "tsc --watch --noEmit --pretty",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\" --ignore-path .gitignore --ignore-path .prettierignore",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\" --ignore-path .gitignore --ignore-path .prettierignore",
    "prepare": "husky"
  },
